<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.small.mapper.project.RepaymentExtensionMapper" >
  <select id="selectDelayNotMoneySumByProjectId" resultType="Double" parameterType="Long">
    select sum(delay_not_money) as delayNotMoney
    from t_repayment_extension
    where project_id = #{projectId} and delay_not_money &gt; 0
  </select>

</mapper>